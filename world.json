data=[{"headline": "Army chief Sisi in focus as Egyptians vote on constitution", "lon": "31.2436663", "weblink": "http://us.mobile.reuters.com/shortArticle/worldNews/idUSBREA0D09520140114", "location": "CAIRO ", "lat": "30.0488185", "short_text": "CAIRO (Reuters) - Egyptians voted on Tuesday for the first time since the military deposed president Mohamed Mursi on a draft constitution that may set the stage for a presidential bid by army chief General Abdel Fattah al-Sisi."}, {"headline": "Syrian government forces advance as rebel infighting rages", "lon": "35.47843", "weblink": "http://us.mobile.reuters.com/shortArticle/worldNews/idUSBREA0D0NM20140114", "location": "BEIRUT ", "lat": "33.8959203", "short_text": "BEIRUT (Reuters) - The Syrian government has retaken territory around the northern city of Aleppo, the military said on Tuesday, after two weeks of rebel infighting that has weakened the insurgency against President Bashar al-Assad."}, {"headline": "Iraq violence kills 24, militants stage big attacks near Falluja", "lon": "44.3787992", "weblink": "http://us.mobile.reuters.com/shortArticle/worldNews/idUSBREA0D19P20140114", "location": "BAGHDAD ", "lat": "33.3024248", "short_text": "BAGHDAD (Reuters) - Sunni Muslim militants staged coordinated attacks near the western Iraqi city of Falluja on Tuesday, destroying two army tanks and capturing a police station, police said."}, {"headline": "Israeli defense chief calls Kerry messianic, drawing U.S. ire", "lon": "35.2247239", "weblink": "http://us.mobile.reuters.com/shortArticle/worldNews/idUSBREA0D0IY20140114", "location": "JERUSALEM ", "lat": "31.7774287", "short_text": "JERUSALEM (Reuters) - Israel's hawkish defense minister drew rare U.S. condemnation on Tuesday over reports he had denounced Secretary of State John Kerry's quest for Israeli-Palestinian peace as messianic and obsessive."}, {"headline": "Hollande stonewalls on private life to make reform pitch", "lon": "2.3521334", "weblink": "http://us.mobile.reuters.com/shortArticle/worldNews/idUSBREA0908G20140114", "location": "PARIS ", "lat": "48.8565056", "short_text": "PARIS (Reuters) - President Francois Hollande brushed away questions about an alleged affair with an actress on Tuesday and unveiled moves to ease company taxes, cut labor charges and trim France's high public spending to revive a stagnant economy."}, {"headline": "Russia denies visa to U.S. journalist critical of Putin", "lon": "37.6187012", "weblink": "http://us.mobile.reuters.com/shortArticle/worldNews/idUSBREA0D0AH20140114", "location": "MOSCOW ", "lat": "55.7516147", "short_text": "MOSCOW (Reuters) - Russia has barred a U.S. journalist critical of President Vladimir Putin from the country for five years, in a move that could upset relations with the United States and has echoes of the Cold War."}, {"headline": "Scores fleeing South Sudan fighting drown in Nile: officials", "lon": "31.5972485", "weblink": "http://us.mobile.reuters.com/shortArticle/worldNews/idUSBREA0D1J920140114", "location": "JUBA ", "lat": "4.8449774", "short_text": "JUBA (Reuters) - Scores of South Sudanese drowned when a packed ferry capsized in the White Nile near the capital of an oil-producing region where government forces and rebels have fought for control, officials said on Tuesday."}, {"headline": "Myanmar still using rape as weapon of war: women's group", "lon": "-120.2126139", "weblink": "http://us.mobile.reuters.com/shortArticle/worldNews/idUSBREA0D1I020140114", "location": "WASHINGTON ", "lat": "47.2868352", "short_text": "WASHINGTON (Reuters) - A women's group says that Myanmar's military is still using rape as a weapon of war, with more than 100 women and girls raped by the army since a 2010 election brought about a nominally civilian government that has pursued rapprochement with the West."}, {"headline": "Three years after uprising, Tunisians celebrate near-model transition", "lon": "8.91972381599097", "weblink": "http://us.mobile.reuters.com/shortArticle/worldNews/idUSBREA0D1DQ20140114", "location": "TUNIS ", "lat": "33.9220712", "short_text": "TUNIS (Reuters) - Thousands of Islamist and secular Tunisians marked the third anniversary of autocrat Zine el-Abidine Ben Ali's downfall on Tuesday, celebrating in the closest the Arab world has to a model transition to democracy."}, {"headline": "Panama canal threatens to take over key expansion project", "lon": "-85.6545729", "weblink": "http://us.mobile.reuters.com/shortArticle/worldNews/idUSBREA0D1I220140114", "location": "PANAMA CITY ", "lat": "30.1600827", "short_text": "PANAMA CITY (Reuters) - The Panama Canal Authority (PCA) said on Tuesday it might take over a key part of the waterway's expansion if the consortium in charge of the project makes good on a threat to suspend work."}, {"headline": "Two students hurt in New Mexico school shooting, suspect in custody", "lon": "None", "weblink": "http://us.mobile.reuters.com/shortArticle/worldNews/idUSBREA0D11Q20140114", "location": "", "lat": "None", "short_text": "(Reuters) - A shooter opened fire at a middle school in New Mexico on Tuesday, seriously wounding at least two teenage students before being taken into custody, police and hospital officials said."}, {"headline": "Franco-German energy firm seen focused on renewables: sources", "lon": "2.3521334", "weblink": "http://us.mobile.reuters.com/shortArticle/worldNews/idUSBREA0D1EX20140114", "location": "PARIS ", "lat": "48.8565056", "short_text": "PARIS (Reuters) - A French-German joint initiative in the energy sector announced by President Francois Hollande is likely to focus on renewable energy but is not aimed at forging alliances between major utilities firms, political and industrial sources said."}, {"headline": "French president's White House trip still on despite reported affair", "lon": "-120.2126139", "weblink": "http://us.mobile.reuters.com/shortArticle/worldNews/idUSBREA0D1EJ20140114", "location": "WASHINGTON ", "lat": "47.2868352", "short_text": "WASHINGTON (Reuters) - French President Francois Hollande's state visit to the United States is still on schedule despite the reported problems that have erupted in Hollande's personal life."}, {"headline": "Militiamen briefly storm Libyan parliament to demand vote", "lon": "13.1787929", "weblink": "http://us.mobile.reuters.com/shortArticle/worldNews/idUSBREA0D1DL20140114", "location": "TRIPOLI ", "lat": "32.8955292", "short_text": "TRIPOLI (Reuters) - Former Libyan militia fighters stormed parliament on Tuesday and fired in the air to try to force assembly members to take a no-confidence vote against Prime Minister Ali Zeidan."}, {"headline": "France to cut 50 billion euros in spending in 2015-17: Hollande", "lon": "2.3521334", "weblink": "http://us.mobile.reuters.com/shortArticle/worldNews/idUSBREA0D19F20140114", "location": "PARIS ", "lat": "48.8565056", "short_text": "PARIS (Reuters) - French President Francois Hollande said on Tuesday he would cut public spending by 50 billion euros ($68 billion) in 2015-17 and ease the tax burden on companies by phasing out hefty family welfare payroll charges."}, {"headline": "White House denounces reported comments about Kerry from Israel defense minister", "lon": "-120.2126139", "weblink": "http://us.mobile.reuters.com/shortArticle/worldNews/idUSBREA0D1B220140114", "location": "WASHINGTON ", "lat": "47.2868352", "short_text": "WASHINGTON (Reuters) - The White House on Tuesday denounced reported comments by Israel's defense minister that were sharply critical of U.S. Secretary of State John Kerry."}, {"headline": "Mexico vigilantes refuse to lay down arms in troubled state", "lon": "-87.8887147", "weblink": "http://us.mobile.reuters.com/shortArticle/worldNews/idUSBREA0D1AD20140114", "location": "NUEVA ITALIA, Mexico ", "lat": "20.5499366", "short_text": "NUEVA ITALIA, Mexico (Reuters) - Vigilante groups battling a powerful drug cartel in a troubled region of Mexico on Tuesday rejected a government call to lay down their arms, raising the risk of an increased security headache for President Enrique Pena Nieto."}, {"headline": "U.S. condemns Iran decision to put wreath on Hezbollah leader's grave", "lon": "-120.2126139", "weblink": "http://us.mobile.reuters.com/shortArticle/worldNews/idUSBREA0D1A520140114", "location": "WASHINGTON ", "lat": "47.2868352", "short_text": "WASHINGTON (Reuters) - The United States on Tuesday condemned a decision by Iran's foreign minister to place a wreath at the grave of a former Lebanese Hezbollah leader accused of perpetrating attacks that killed hundreds of people."}, {"headline": "British phone-hacking trial shown footage of alleged Brooks' cover-up", "lon": "-0.1276597", "weblink": "http://us.mobile.reuters.com/shortArticle/worldNews/idUSBREA0D0SL20140114", "location": "LONDON ", "lat": "51.5072759", "short_text": "LONDON (Reuters) - The trial of Rebekah Brooks, who ran Rupert Murdoch's British newspaper arm, was shown film footage on Tuesday of what the prosecution alleges was her husband's attempt to hide evidence from police investigating accusations of phone-hacking."}, {"headline": "Indian diplomat seeks dismissal of U.S. criminal case", "lon": "-73.9865812", "weblink": "http://us.mobile.reuters.com/shortArticle/worldNews/idUSBREA0D18U20140114", "location": "NEW YORK ", "lat": "40.7305991", "short_text": "NEW YORK (Reuters) - A lawyer for Indian diplomat Devyani Khobragade on Tuesday asked a U.S. judge to throw out immigration and employment charges against her that led to a major diplomatic spat between the United States and India."},{"headline": "French president's White House trip still on despite reported affair", "lon": "-120.2126139", "weblink": "http://us.mobile.reuters.com/shortArticle/worldNews/idUSBREA0D1EJ20140114", "location": "WASHINGTON ", "lat": "47.2868352", "short_text": "WASHINGTON (Reuters) - French President Francois Hollande's state visit to the United States is still on schedule despite the reported problems that have erupted in Hollande's personal life."}, {"headline": "Militiamen briefly storm Libyan parliament to demand vote", "lon": "13.1787929", "weblink": "http://us.mobile.reuters.com/shortArticle/worldNews/idUSBREA0D1DL20140114", "location": "TRIPOLI ", "lat": "32.8955292", "short_text": "TRIPOLI (Reuters) - Former Libyan militia fighters stormed parliament on Tuesday and fired in the air to try to force assembly members to take a no-confidence vote against Prime Minister Ali Zeidan."}, {"headline": "France to cut 50 billion euros in spending in 2015-17: Hollande", "lon": "2.3521334", "weblink": "http://us.mobile.reuters.com/shortArticle/worldNews/idUSBREA0D19F20140114", "location": "PARIS ", "lat": "48.8565056", "short_text": "PARIS (Reuters) - French President Francois Hollande said on Tuesday he would cut public spending by 50 billion euros ($68 billion) in 2015-17 and ease the tax burden on companies by phasing out hefty family welfare payroll charges."}, {"headline": "White House denounces reported comments about Kerry from Israel defense minister", "lon": "-120.2126139", "weblink": "http://us.mobile.reuters.com/shortArticle/worldNews/idUSBREA0D1B220140114", "location": "WASHINGTON ", "lat": "47.2868352", "short_text": "WASHINGTON (Reuters) - The White House on Tuesday denounced reported comments by Israel's defense minister that were sharply critical of U.S. Secretary of State John Kerry."}]
